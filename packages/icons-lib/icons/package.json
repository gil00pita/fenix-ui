{
  "name": "@fenix-ui/icons",
  "version": "0.0.1",
  "description": "Fork of https://github.com/react-icons/react-icons a SVG React icons of popular icon packs using ES6 imports",
  "author": "Gil Alvaro",
  "contributors": [],
  "license": "MIT",
  "main": "lib",
  "types": "./lib/esm/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "watch": "tsc -w -p tsconfig.json",
    "build": "node scripts/build.js && yarn build-mjs && yarn build-cjs",
    "prebuild": "node ../../../node_modules/copy/bin/cli.js ../../../README.md ./packages/icons-lib/icons && rimraf ./lib",
    "build-mjs": "tsc && babel ./lib/esm -d ./lib/esm",
    "build-cjs": "tsc -p ./tsconfig.commonjs.json",
    "clean": "rimraf ./src/icons/MaterialDesign-SVG && rimraf ./src/icons/ant-design-icons"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:react-icons/react-icons.git"
  },
  "bugs": {
    "url": "https://github.com/react-icons/react-icons/issues"
  },
  "homepage": "https://github.com/react-icons/react-icons#readme",
  "peerDependencies": {
    "react": "17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@mdi/svg": "^5.9.55",
    "@types/react": "^17.0.9",
    "babel-eslint": "^10.1.0",
    "camelcase": "^6.2.0",
    "cheerio": "1.0.0-rc.10",
    "copy": "^0.3.2",
    "eslint": "^7.28.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "find-package": "^1.0.0",
    "glob-promise": "^4.2.0",
    "lerna": "^4.0.0",
    "prettier": "^2.3.1",
    "renamer": "^3.1.1",
    "rimraf": "^3.0.2",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "camelcase": "^6.2.0"
  }
}
